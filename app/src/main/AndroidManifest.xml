<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gx.accountbooks">


    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />


    <application
        android:name="com.gx.accountbooks.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.Note"
        android:usesCleartextTraffic="true"
        tools:replace="android:name,android:roundIcon,android:icon">

        <activity android:name="com.gx.accountbooks.HomeActivity">

        </activity>
        <activity android:name="com.gx.accountbooks.test.TestActivity">

        </activity>

        <activity android:name="com.gx.module_login.mvp.ui.login.LoginActivity" />

        <activity
            android:name=".test.activity.SingleTaskActivity"
            android:launchMode="singleTask">

        </activity>

        <activity
            android:name=".test.activity.SingleTopActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".test.activity.Bluetooth">

        </activity>

        <activity android:name=".test.activity.FirstActivity">

        </activity>

        <activity
            android:name=".test.activity.SingleInstanceActivity"
            android:launchMode="singleInstance">

        </activity>
    </application>

</manifest>